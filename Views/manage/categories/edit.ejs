<h3>Add Category</h3>
<form action="/manage/categories/edit/<%= category._id %> " method="post">
  <div class="form-group row">
    <label for=""
      >Name
      <input
        class="form-control col-sm-12"
        type="text"
        name="name"
        value="<%= category.name %>"
      />
    </label>
  </div>
  <div class="form-group row">
    <div class="col-sm-12">
      <input type="submit" class="btn btn-primary" value="submit" />
    </div>
  </div>
</form>
